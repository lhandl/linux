set background=dark
set number
set ruler
set noerrorbells
set autoindent
set nosmartindent
set expandtab tabstop=2 softtabstop=2 shiftwidth=2
set wrap
highlight Normal ctermfg=grey ctermbg=black

" ---- Insert Timestamp on <F5> press
nnoremap <F5> "=strftime("%s")<CR>P
inoremap <F5> <C-R>=strftime("%s")<CR>
